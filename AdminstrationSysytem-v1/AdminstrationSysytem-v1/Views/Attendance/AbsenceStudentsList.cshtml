@model IEnumerable<AdminstrationSysytem_v1.Models.AttendanceModel>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayName("No of Permissions")
        </th>
        <th>
            @Html.DisplayName("Grade of Absence")
        </th>
        <th>
            @Html.DisplayName("Date")
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoOfPermissions)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GradeOfAbsence)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
        </tr>
    }

</table>
