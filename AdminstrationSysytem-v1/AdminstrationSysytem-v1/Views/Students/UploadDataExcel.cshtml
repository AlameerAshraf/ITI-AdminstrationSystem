
<div class="container">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><i class="fa fa-file-excel-o" aria-hidden="true"></i> Upload Data From External Source </h4>
            </div>
            <div class="modal-body">
                <form action="/Students/UploadDataExcel" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <label for="Birthdate" class="col-sm-2 control-label">File</label>
                        <div>
                            <input accept=".xls,.xlsx" type="file" class="form-control" name="FileExcel" placeholder="Birthdate" id="FileExcel">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button type="submit" class="btn btn-danger">Upload!</button>
                        </div>
                    </div>
                </form>
            </div>
            <br />
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>



